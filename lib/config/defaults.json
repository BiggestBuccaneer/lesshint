{
    "attributeQuotes": {
        "enabled": true,
        "style": "single"
    },

    "borderZero": {
        "enabled": true,
        "style": "zero"
    },

    "comment": {
        "allowed": "^!",
        "enabled": true
    },

    "duplicateProperty": {
        "enabled": true,
        "exclude": []
    },

    "emptyRule": {
        "enabled": true
    },

    "finalNewline": {
        "enabled": true
    },

    "hexLength": {
        "enabled": true,
        "style": "long"
    },

    "hexNotation": {
        "enabled": true,
        "style": "lowercase"
    },

    "hexValidation": {
        "enabled": true
    },

    "idSelector": {
        "enabled": true,
        "exclude": []
    },

    "importantRule": {
        "enabled": true
    },

    "leadingZero": {
        "enabled": true,
        "style": "include_zero"
    },

    "qualifyingElement": {
        "enabled": true,
        "allowWithId": false,
        "allowWithClass": false,
        "allowWithAttribute": false
    },

    "spaceAfterPropertyColon": {
        "enabled": true,
        "style": "one_space"
    },

    "spaceAfterPropertyName": {
        "enabled": true,
        "style": "no_space"
    },

    "spaceAfterPropertyValue": {
        "enabled": true,
        "style": "no_space"
    },

    "spaceBeforeBrace": {
        "enabled": true,
        "style": "one_space"
    },

    "stringQuotes": {
        "enabled": true,
        "style": "single"
    },

    "trailingSemicolon": {
        "enabled": true
    },

    "trailingZero": {
        "enabled": true,
        "style": "exclude_zero"
    },

    "urlFormat": {
        "enabled": true,
        "style": "relative"
    },

    "urlQuotes": {
        "enabled": true
    },

    "zeroUnit": {
        "enabled": true,
        "style": "no_unit"
    }
}
